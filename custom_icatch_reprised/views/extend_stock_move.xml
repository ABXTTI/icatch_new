<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="customize_stock_picking_form_1" model="ir.ui.view">
        <field name="name">customize.stock.picking.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//tree/field[@name='product_id']" position="after">
                <field name="i_shop"/>
                <field name="i_description"/>
                <field name="i_city"/>
                <field name="i_mediadescription"/>
                <field name="i_medium_description"/>
                <field name="i_width"/>
                <field name="i_sqrfeet"/>
                <field name="i_qty"/>
                <field name="i_totalsqrfeet"/>
                <field name="map_link"/>
            </xpath>
        </field>
    </record>
</odoo>